{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../public/dist/",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "../public/dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": 744,
  "autoInstall": false,
  "rtl": false,
  "components": ["reset", "site", "button", "container", "header", "icon", "image", "input", "label", "segment", "form", "grid", "menu", "message", "table", "card", "feed", "checkbox", "dropdown", "search", "tab", "api", "form", "state", "visibility", "transition"],
  "version": "2.3.3"
}
